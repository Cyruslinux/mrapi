// This file to fix pmt pnpm bug

datasource management {
  provider = ["sqlite", "mysql", "postgresql"]
  url      = env("MANAGEMENT_URL")
}

generator client {
  provider      = "prisma-client-js"
  output        = env("MANAGEMENT_OUTPUT")
  binaryTargets = ["native"]
}

model Tenant {
  name String @id
  url  String
}
