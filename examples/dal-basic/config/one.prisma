model User {
  email String  @unique
  id    Int     @id @default(autoincrement())
  name  String?
  // timesssss DateTime
  // jsontest  Json
  Post  Post[]
}

model Post {
  authorId  Int?
  content   String?
  id        Int     @id @default(autoincrement())
  published Boolean @default(false)
  title     String
  User      User?   @relation(fields: [authorId], references: [id])
}
