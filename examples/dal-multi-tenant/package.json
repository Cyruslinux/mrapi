{
  "name": "dal-multi-tenant",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "prod": "cross-env NODE_ENV=production node dist/src/app",
    "build": "rm -rf dist && npm run generate && tsc",
    "clean": "rm -rf dist && rm -rf node_modules",
    "generate": "npx mrapi generate --services management blog music",
    "migrate": "npm run migrate:management && npm run migrate:services",
    "migrate:management": "npx mrapi migrate --services management",
    "migrate:services": "npx mrapi migrate --services blog music",
    "dmmf": "ts-node-dev --respawn --transpile-only ./scripts/dmmf.ts",
    "gen1-eqm": "npx mrapi generate --name one --eqm findOne,createOne --em Post"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mrapi/dal": "workspace:*",
    "@mrapi/cli": "workspace:*",
    "cross-env": "^7.0.2",
    "@prisma/cli": "^2.9.0",
    "@nexus/schema": "0.16.0"
  },
  "devDependencies": {
    "tslib": "^2.0.3",
    "fs-extra": "^9.0.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "ts-node-dev": "1.0.0-pre.63"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
